<div id="posts" class="transitions-enabled">
  <% @posts.each do |post| %>
  <div class="box panel panel-default">
    <%= link_to (image_tag post.image.url), post %>
    <h2><%= link_to post.title, post %></h2>
    <p class="user">Submitted by <%= post.user.email %></p>
  </div>
  <% end %>
</div>
